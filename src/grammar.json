{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "taobin_language",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "_main"
    },
    "_main": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_tag_block"
          },
          {
            "type": "SYMBOL",
            "name": "_statement"
          }
        ]
      }
    },
    "_tag_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "tag_open",
          "content": {
            "type": "PATTERN",
            "value": "<\\w+>"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "FIELD",
          "name": "tag_close",
          "content": {
            "type": "PATTERN",
            "value": "<\\/w+>"
          }
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "element_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "event_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "variable_assignment"
        },
        {
          "type": "SYMBOL",
          "name": "conditional_statement"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "include_statement"
        }
      ]
    },
    "element_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "tag_open",
          "content": {
            "type": "PATTERN",
            "value": "<\\w+>"
          }
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_literal"
              },
              {
                "type": "SYMBOL",
                "name": "number_literal"
              },
              {
                "type": "SYMBOL",
                "name": "variable_reference"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "tag_close",
          "content": {
            "type": "PATTERN",
            "value": "<\\/\\w+>"
          }
        }
      ]
    },
    "event_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "tag_open",
          "content": {
            "type": "PATTERN",
            "value": "<(EventOpen|EventTimeout|EventSecurity|EventMachine|EventClick|EventUnitTest4)>"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_event_statement"
          }
        },
        {
          "type": "FIELD",
          "name": "tag_close",
          "content": {
            "type": "PATTERN",
            "value": "<\\/(EventOpen|EventTimeout|EventSecurity|EventMachine|EventClick|EventUnitTest4)>"
          }
        }
      ]
    },
    "_event_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_assignment"
        },
        {
          "type": "SYMBOL",
          "name": "conditional_statement"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "function_call"
        },
        {
          "type": "SYMBOL",
          "name": "open_statement"
        }
      ]
    },
    "variable_assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Var"
        },
        {
          "type": "FIELD",
          "name": "variable_name",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "SYMBOL",
                "name": "path_identifier"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_literal"
              },
              {
                "type": "SYMBOL",
                "name": "number_literal"
              },
              {
                "type": "SYMBOL",
                "name": "variable_reference"
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              }
            ]
          }
        }
      ]
    },
    "path_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable_reference"
            },
            {
              "type": "STRING",
              "value": "+"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "variable_reference"
                },
                {
                  "type": "SYMBOL",
                  "name": "string_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "number_literal"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable_reference"
            },
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "variable_reference"
                },
                {
                  "type": "SYMBOL",
                  "name": "number_literal"
                }
              ]
            }
          ]
        }
      ]
    },
    "conditional_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "If"
        },
        {
          "type": "FIELD",
          "name": "condition",
          "content": {
            "type": "SYMBOL",
            "name": "condition"
          }
        },
        {
          "type": "STRING",
          "value": "Then"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_event_statement"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "Else"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_event_statement"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "EndIf"
        }
      ]
    },
    "condition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable_reference"
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "string_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "number_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "variable_reference"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable_reference"
            },
            {
              "type": "STRING",
              "value": ">"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "number_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "variable_reference"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable_reference"
            },
            {
              "type": "STRING",
              "value": ">="
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "number_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "variable_reference"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable_reference"
            },
            {
              "type": "STRING",
              "value": "<"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "number_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "variable_reference"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable_reference"
            },
            {
              "type": "STRING",
              "value": "<="
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "number_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "variable_reference"
                }
              ]
            }
          ]
        }
      ]
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "INT"
            },
            {
              "type": "STRING",
              "value": "SPLIT"
            },
            {
              "type": "STRING",
              "value": "DEBUGVAR"
            },
            {
              "type": "STRING",
              "value": "TimerReset"
            },
            {
              "type": "STRING",
              "value": "Refresh"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "parameters",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "variable_reference"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "string_literal"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "number_literal"
                          }
                        ]
                      },
                      {
                        "type": "REPEAT",
                        "content": {
                          "type": "SEQ",
                          "members": [
                            {
                              "type": "STRING",
                              "value": ","
                            },
                            {
                              "type": "CHOICE",
                              "members": [
                                {
                                  "type": "SYMBOL",
                                  "name": "variable_reference"
                                },
                                {
                                  "type": "SYMBOL",
                                  "name": "string_literal"
                                },
                                {
                                  "type": "SYMBOL",
                                  "name": "number_literal"
                                }
                              ]
                            }
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "open_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Open"
        },
        {
          "type": "FIELD",
          "name": "target",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_literal"
              },
              {
                "type": "SYMBOL",
                "name": "variable_reference"
              }
            ]
          }
        }
      ]
    },
    "include_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";include="
        },
        {
          "type": "FIELD",
          "name": "target",
          "content": {
            "type": "SYMBOL",
            "name": "string_literal"
          }
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_]*"
    },
    "variable_reference": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "Var("
            },
            {
              "type": "FIELD",
              "name": "variable_name",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "identifier"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "path_identifier"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\"]*"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "number_literal": {
      "type": "PATTERN",
      "value": "[0-9]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
